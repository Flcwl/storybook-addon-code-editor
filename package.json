{
  "name": "storybook-addon-code-editor",
  "version": "1.0.0",
  "description": "A Storybook add-on for live editing stories.",
  "repository": "https://github.com/JeremyRH/storybook-addon-code-editor.git",
  "author": "JeremyRH <holcomb227@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "*.js"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "scripts": {
    "build": "tsc --outDir dist/es --module esnext && tsc --outDir dist/cjs --module commonjs --declaration false",
    "docs": "cd example && npm run build-storybook",
    "format": "prettier src example/src --write",
    "install-example-deps": "cd example && npm ci",
    "start": "npm run build && cd example && npm run storybook",
    "test": "jest src"
  },
  "dependencies": {
    "@babel/standalone": "^7.21.2",
    "@storybook/addons": "^6.5.16",
    "@storybook/components": "^6.5.16",
    "copy-webpack-plugin": "^11.0.0",
    "monaco-editor": "^0.36.1",
    "raw-loader": "^4.0.2"
  },
  "peerDependencies": {
    "@types/react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5"
  }
}
