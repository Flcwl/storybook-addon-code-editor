{
  "name": "storybook-addon-code-editor",
  "version": "0.3.4",
  "description": "A Storybook add-on for live editing stories.",
  "repository": "https://github.com/JeremyRH/storybook-addon-code-editor.git",
  "author": "JeremyRH <holcomb227@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "register.js"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "scripts": {
    "build": "tsc --outDir dist/es --module esnext && tsc --outDir dist/cjs --module commonjs --declaration false",
    "docs": "cd example && npm run build-storybook",
    "format": "prettier src example/src --write",
    "install-example-deps": "cd example && npm ci",
    "start": "npm run build && cd example && npm run storybook",
    "test": "jest src"
  },
  "dependencies": {
    "@babel/standalone": "7",
    "@storybook/addons": "6",
    "@storybook/components": "6",
    "@types/react": "16.9^ || 17 || 18",
    "monaco-editor": "^0.33.0",
    "react": "16.9^ || 17 || 18",
    "react-dom": "16.9^ || 17 || 18"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "copy-webpack-plugin": "^6.4.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.7.4"
  }
}
